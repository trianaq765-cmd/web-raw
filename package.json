{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/paste",
      "dest": "/api/paste.js"
    },
    {
      "src": "/api/raw/([^/]+)",
      "dest": "/api/raw/[id].js?id=$1"
    },
    {
      "src": "/api/([^/]+)",
      "dest": "/api/[id].js?id=$1"
    },
    {
      "src": "/raw/([^/]+)",
      "dest": "/api/raw/[id].js?id=$1"
    },
    {
      "src": "/view/([^/]+)",
      "dest": "/public/view.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
